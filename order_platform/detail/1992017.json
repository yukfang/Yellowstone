      "blocked_roles": [],
      "items": [
        {
          "content": "<p>Hi Robbie, do you expect any negative impact on campaign performance for the next few weeks while we wait for them to fix the deduplication issue?&nbsp;</p>",
          "type": 6
        }
      ],
      "avatar": "https://pan16.larksuitecdn.com/static-resource/v1/v2_44848684-7ba2-4d22-905b-f950a19aab2h~?image_size=72x72&cut_type=&quality=&format=png&sticker_format=.webp",
      "context": {}
    },
    {
      "is_hide_reply": false,
      "create_time": 1712333431,
      "auto_close_hit": 0,
      "id": 7354415384063164000,
      "reply_role": null,
      "staff_id": "",
      "type": 0,
      "is_questioner": false,
